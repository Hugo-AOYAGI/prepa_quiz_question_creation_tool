{
  "name": "quiz-app-database",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder build"
  },
  "build": {
    "appId": "yourappid",
    "asar": true,
    "asarUnpack": [
      "configuration/**/*",
      "output/**/*",
      "appdata/**/*",
      "node_modules/easy-pdf-merge/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "directories": {
      "buildResources": "build_assets",
      "output": "release"
    },
    "win": {
      "target": "NSIS",
      "icon": "build_assets/icon.ico"
    }
  },
  "author": "Hugo AOYAGI",
  "license": "ISC",
  "dependencies": {
    "html2canvas": "^1.0.0-rc.5",
    "imagemin": "^7.0.1",
    "imagemin-pngquant": "^8.0.0",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "electron": "^13.6.6",
    "electron-builder": "^22.5.1",
    "electron-packager": "^14.2.1",
    "grunt-contrib-imagemin": "^4.0.0"
  }
}
